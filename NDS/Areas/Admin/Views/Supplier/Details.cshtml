@model Supplier

@using NDS.Utility


@{
    Layout = null;
}


<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">Supplier Details For "@Model.Title"</h4>
</div>


<div class="modal-body form-horizontal" style="padding:24px;">

    <div class="form-group">

        <label asp-for="CompanyName" class="control-label"></label>

        <p>
            @Model.CompanyName
        </p>

    </div>

    <hr />


    <div class="form-group">

        <label asp-for="Tel" class="control-label"></label>

        <p>
            @Model.Tel
        </p>

    </div>

    <hr />



    <div class="form-group">

        <label asp-for="Address" class="control-label"></label>

        <p>
            @Model.Address
        </p>

    </div>

    <hr />



    <div class="form-group">

        <label asp-for="FkProvinceId" class="control-label"></label>

        <p>
            @Model.Tbl_Province.Title
        </p>

    </div>

    <hr />



    <div class="form-group">

        <label asp-for="FkCityId" class="control-label"></label>

        <p>
            @Model.Tbl_City.Title
        </p>

    </div>

    <hr />

    <div class="form-group">

        <label asp-for="CreateDate" class="control-label"></label>

        <p>
            @Model.CreateDate
        </p>

    </div>

    <hr />




</div>


<div class="modal-footer" style="text-align:left;">

    <button class="btn btn-default" type="button" data-dismiss="modal">Close</button>


</div>





