@model AdminUser

@using NDS.Utility



@{
    Layout = null;
}


<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">AdminUser Details For "@Model.FirstName @Model.LastName"</h4>
</div>


<div class="modal-body form-horizontal" style="padding:24px;">




    <div class="form-group">

        <label asp-for="UserName" class="control-label"></label>


        <p>@Model.UserName</p>

    </div>

    <hr />



    <div class="form-group">

        <label asp-for="Phone" class="control-label"></label>


        <p>@Model.Phone</p>

    </div>

    <hr />




    <div class="form-group">

        <label asp-for="Email" class="control-label"></label>


        <p>@Model.Email</p>

    </div>

    <hr />





    <div class="form-group">

        <label asp-for="BirthDate" class="control-label"></label>


        <p>@Model.BirthDate</p>

    </div>

    <hr />


    <div class="form-group">

        <label asp-for="Gender" class="control-label"></label>



        @if (Model.Gender == Gender.Male.ToInt())
        {

            <p>
                Male
            </p>
        }
        else if (Model.Gender == Gender.FeMale.ToInt())
        {
            <p>
                FeMale
            </p>
        }
        else if (Model.Gender == Gender.Other.ToInt())
        {
            <p>
                Other
            </p>
        }

    </div>

    <hr />



</div>


<div class="modal-footer" style="text-align:left;">

    <button class="btn btn-default" type="button" data-dismiss="modal">Close</button>


</div>





